      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "55ead427-05d5-4063-874e-e032eada470c"
      }
    },
    "owner": "lo10th",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/55ead427-05d5-4063-874e-e032eada470c"
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera",
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone",
          "recordAudioAndroid": true
        }
      ]
    ]
